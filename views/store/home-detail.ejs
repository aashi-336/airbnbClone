<%- include('../partials/head')  %>
  </head>
  <body>
    <%- include('../partials/nav')  %>
  
    <main class="container mx-auto bg-white shadow-lg rounded-lg p-8 mt-10 max-w-xl">
      <h2 class="text-5xl text-gray-800 font-bold text-center mb-6">You are in home detail page </h2>
      <li class="p-4 bg-gray-100 rounded-lg shadow-md">
        <div class="flex flex-col items-center">
          <img src="<%= home.photoUrl%>" alt="<%= home.houseName %>" class="w-full h-48 object-cover rounded-lg mb-4">
          <h3 class="text-2xl font-bold text-gray-800"><%= home.houseName %></h3>
          <p class="text-gray-600">Location: <%= home.location %></p>
          <p class="text-gray-600">Price: $<%= home.price %></p>
          <p class="text-gray-600">Rating: <%= home.rating %> / 5</p>
          <button class="bg-cyan-500 p-2">Book</button>
          <div class="mt-2">
            <%- include('../partials/favourite')  %>
          </div>
        </div>
        
      </li>
    </main>
  </body>
</html>
